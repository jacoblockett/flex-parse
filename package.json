{
	"name": "flex-parse",
	"version": "0.3.0",
	"description": "A flexible document/ml parser, allowing non-standard practices and preferring a greedy 'preserve all' approach",
	"main": "dist/index.js",
	"module": "src/index.js",
	"exports": {
		"require": "./dist/index.cjs",
		"import": "./src/index.js"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/jacoblockett/flex-parse"
	},
	"scripts": {
		"dev": "clear && node dev/sandbox.js",
		"test": "node --test tests/test.js",
		"build": "rollup -c"
	},
	"keywords": [
		"html",
		"xml",
		"parse",
		"parser",
		"flexible",
		"greedy",
		"preserve",
		"dom",
		"vdom"
	],
	"author": "Jacob Lockett",
	"license": "MIT",
	"devDependencies": {
		"@rollup/plugin-commonjs": "^29.0.0",
		"@rollup/plugin-node-resolve": "^16.0.3",
		"rollup": "^4.53.2",
		"zod": "^3.24.1"
	},
	"dependencies": {
		"virty": "github:jacoblockett/virty"
	}
}
